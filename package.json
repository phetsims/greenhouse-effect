{
  "name": "greenhouse-effect",
  "version": "1.3.0-dev.0",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/phetsims/greenhouse-effect.git"
  },
  "devDependencies": {
    "grunt": "~1.5.3"
  },
  "phet": {
    "requirejsNamespace": "GREENHOUSE_EFFECT",
    "phetLibs": [
      "nitroglycerin",
      "bamboo"
    ],
    "simulation": true,
    "runnable": true,
    "supportedBrands": [
      "phet",
      "phet-io",
      "adapted-from-phet"
    ],
    "simFeatures": {
